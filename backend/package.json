    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
